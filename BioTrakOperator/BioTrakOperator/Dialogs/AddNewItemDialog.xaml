<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BioTrakOperator.Pages.AddNewItemDialog">
    <ContentPage.Content>
        <StackLayout>
            <Image Source="logo.png" WidthRequest="250" Margin="0,10,0,20"/>

            <Button Text="Foto" Clicked="AddNewPhotoObject" BackgroundColor="Transparent" BorderColor="#2196F3" BorderWidth="2" CornerRadius="10" TextColor="#2196F3"/>
            <Button Text="ID oggetto" Clicked="AddNewTextObject" BackgroundColor="Transparent" BorderColor="#2196F3" BorderWidth="2" CornerRadius="10" TextColor="#2196F3"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>