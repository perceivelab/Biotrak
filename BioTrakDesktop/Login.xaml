<Window x:Class="BioTrakDesktop.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BioTrakDesktop"
        mc:Ignorable="d"
        Title="Login" Height="112" Width="314">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="41*"/>
            <RowDefinition Height="55*"/>
        </Grid.RowDefinitions>
        <TextBox HorizontalAlignment="Left" Margin="37,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Name="txtUser" Width="229" Height="22" GotFocus="TextBox_GotFocus" LostFocus="TextBox_LostFocus"/>
        <TextBox HorizontalAlignment="Left" Margin="37,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Name="txtPassword" Width="229" Height="22" Grid.Row="1" GotFocus="TextBox_GotFocus" LostFocus="TextBox_LostFocus"/>

    </Grid>
</Window>
